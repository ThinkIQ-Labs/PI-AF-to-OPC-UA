<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:annotation>
    <xs:documentation xml:language="en">
      OSIsoft.AF Schema, Copyright (c) 2008-2009 OSIsoft, LLC. All rights reserved.
    </xs:documentation>
  </xs:annotation>
  <xs:element name="AF">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="SecurityAccessControl" type="xs:string" minOccurs="0"/>
        <xs:element name="SecurityAccessControlContact" type="xs:string" minOccurs="0"/>
        <xs:element name="SecurityAccessControlDatabase" type="xs:string" minOccurs="0"/>
        <xs:element name="SecurityAccessControlNotificationContactTemplate" type="xs:string" minOccurs="0"/>
        <xs:choice minOccurs="0" maxOccurs ="unbounded">      
          <xs:element ref="AFAdjustment"/>
          <xs:element ref="AFAnalysis"/>
          <xs:element ref="AFAnalysisCategory"/>
          <xs:element ref="AFAnalysisRule"/>
          <xs:element ref="AFAnalysisTemplate"/>
          <xs:element ref="AFAttribute"/>
          <xs:element ref="AFAttributeCategory"/>
          <xs:element ref="AFAttributeCategoryRef"/>
          <xs:element ref="AFAttributeTemplate"/>
          <xs:element ref="AFCase"/>
          <xs:element ref="AFConnection"/>
          <xs:element ref="AFContact"/>
          <xs:element ref="AFContactRef"/>
          <xs:element ref="AFDatabase"/>
          <xs:element ref="AFDeliveryFormat"/>
          <xs:element ref="AFElement"/>
          <xs:element ref="AFElementCategory"/>
          <xs:element ref="AFElementCategoryRef"/>
          <xs:element ref="AFElementRef"/>
          <xs:element ref="AFElementTemplate"/>
          <xs:element ref="AFEnumerationSet"/>
          <xs:element ref="AFEnumerationValue"/>
          <xs:element ref="AFEventFrame"/>
          <xs:element ref="AFLayer"/>
          <xs:element ref="AFModel"/>
          <xs:element ref="AFModelRef"/>
          <xs:element ref="AFModelAnalysis"/>
          <xs:element ref="AFNotification"/>
          <xs:element ref="AFNotificationContact"/>
          <xs:element ref="AFNotificationContactRef"/>
          <xs:element ref="AFNotificationContactTemplate"/>
          <xs:element ref="AFNotificationContactTemplateRef"/>	
          <xs:element ref="AFNotificationContent"/>
          <xs:element ref="AFNotificationRule"/>
          <xs:element ref="AFNotificationRuleCategory"/>
          <xs:element ref="AFNotificationRuleTemplate"/>
          <xs:element ref="AFNotificationTemplate"/>
          <xs:element ref="AFPlugIn"/>
          <xs:element ref="AFPort"/>
          <xs:element ref="AFReferenceType"/>
          <xs:element ref="AFReferenceTypeCategory"/>
          <xs:element ref="AFResult"/>
          <xs:element ref="AFSecurityIdentity"/>
          <xs:element ref="AFSecurityMapping"/>
          <xs:element ref="AFTable"/>
          <xs:element ref="AFTableCategory"/>
          <xs:element ref="AFTableConnection"/>
          <xs:element ref="AFTimeRule"/>
          <xs:element ref="AFTransfer"/>
          <xs:element ref="PISystem"/>
          <xs:element ref="UOM"/>
          <xs:element ref="UOMBaseClass"/>
          <xs:element ref="UOMClass"/>
          <xs:element ref="UOMDatabase"/>
          <xs:element ref="UOMGroup"/>
        </xs:choice>
      </xs:sequence>
      <xs:attribute name="id" type="xs:string" use="optional"/>
      <xs:attribute name="SoftwareVersion" type="xs:string" use="optional"/>
      <xs:attribute name="SchemaVersion" type="xs:string" use="optional"/>
      <xs:attribute name="ParentKey" type="xs:string" use="optional"/>
      <xs:attribute name="PISystem" type="xs:string" use="required"/>
      <xs:attribute name="PIPersist" type="xs:string" use="optional"/>
      <xs:attribute name="Identity" type="xs:string" use="required"/>
      <xs:attribute name="ExportedType" type="xs:string" use="required"/>
      <xs:attribute name="ExportedObject" type="xs:string" use="optional"/>
      <xs:attribute name="ExportMode" type="xs:string" use="optional"/>
      <xs:attribute name="Description" type="xs:string" use="optional"/>
      <xs:attribute name="Database" type="xs:string" use="optional"/>
      <xs:attribute name="Created" type="xs:string" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:group name="AFBaseElement">
    <!-- AllReferences adds 
        AFElementTemplate in Template
        AFElementCategories referenced
        AFEnumerationSets referenced in non templated attributes-->
    <xs:sequence>
      <xs:element ref="id" minOccurs="0"/>
      <xs:element ref="Name" minOccurs="0"/>
      <xs:element ref="Description" minOccurs="0"/>
      <xs:element name="Template" type="xs:string" minOccurs="0"/>
      <xs:element name="IsAnnotated" type="xs:boolean" minOccurs="0"/>
      <xs:element name="DefaultAttribute" type="xs:string" minOccurs="0"/>
      <xs:element name="DefaultInputPort" type="xs:string" minOccurs="0"/>
      <xs:element name="DefaultOutputPort" type="xs:string" minOccurs="0"/>
      <xs:element name="DefaultUndirectedPort" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="AFBaseElementChildren">
    <xs:sequence>
      <xs:element ref="AFExtendedProperty" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element ref="AFElementCategoryRef" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element ref="AFAttribute" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element ref="AFPort" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element ref="AFAnnotation" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="AFNotificationRuleChildren">
    <xs:sequence>
      <xs:element ref="AFNotificationRuleCategoryRef" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element ref="AFDeliveryFormat" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element ref="AFNotificationRuleSubscriber" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="AFCategory">
    <!-- AllReferences adds AFSecurityIdentities from SecurityAccessControl if Security is also exported -->
    <xs:sequence>
      <xs:element ref="id" minOccurs="0"/>
      <xs:element ref="Name"/>
      <xs:element ref="Description" minOccurs="0"/>
      <xs:element name="SecurityAccessControl" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="AFValue">
    <xs:sequence>
      <xs:element name="Status" type="xs:string" minOccurs="0"/>
      <xs:element name="Timestamp" type="xs:dateTime" minOccurs="0"/>
      <xs:element name="UOM" type="xs:string" minOccurs="0"/>
      <xs:element ref="Value" minOccurs="0"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="AFVersion">
    <xs:sequence>
      <xs:element name="VersionID" type="xs:string" minOccurs="0"/>
      <xs:element name="Modifier" type="xs:string" minOccurs="0"/>
      <xs:element name="Comment" type="xs:string" minOccurs="0"/>
      <xs:element name="EffectiveDate" type="xs:dateTime" minOccurs="0"/>
      <xs:element name="ObsoleteDate" type="xs:dateTime" minOccurs="0"/>
    </xs:sequence>
  </xs:group>
  <xs:attribute name="id" type="xs:string"/>
  <xs:attribute name="operation">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="delete"/>
        <xs:enumeration value="remove"/>
        <xs:enumeration value="createconfig"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:attribute>
  <xs:attribute name="ReferenceType" type="xs:string"/>
  <xs:attribute name="UOMGroup" type="xs:string"/>
  <xs:element name="id" type="xs:string"/>
  <xs:element name="Name" type="xs:string"/>
  <xs:element name="Description" type="xs:string"/>
  <xs:element name="SecurityAccessControl" type="xs:string"/>
  <xs:element name="Value">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0">
        <xs:element ref="AFFile" minOccurs = "0"/>
        <xs:element name="Item" minOccurs ="0" maxOccurs ="unbounded"/>
      </xs:choice>
      <xs:attribute name="default" type="xs:boolean" use="optional"/>
      <xs:attribute name="type" type="xs:string" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Target">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs ="unbounded"> 
        <xs:element ref="AFElementRef"/>
        <xs:element ref="AFModelRef"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="ContactType">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="Individual"/>
        <xs:enumeration value="Group"/>
        <xs:enumeration value="Escalation"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="Priority">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="High"/>
        <xs:enumeration value="AboveNormal"/>
        <xs:enumeration value="Normal"/>
        <xs:enumeration value="BelowNormal"/>
        <xs:enumeration value="Low"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="AutoCreate">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="DoNotCreate"/>
        <xs:enumeration value="AutoCreate"/>
        <xs:enumeration value="AutoCreateAndStart"/>
        <xs:enumeration value="Prompt"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="Status">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="NotReady"/>
        <xs:enumeration value="Disabled"/>
        <xs:enumeration value="Enabled"/>
        <xs:enumeration value="Error"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="AFAdjustment">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Creator" type="xs:string" minOccurs="0"/>
        <xs:element name="CreationTime" type="xs:dateTime" minOccurs="0"/>
        <xs:element name="Comment" type="xs:string" minOccurs="0"/>
        <xs:element name="PreviousValue" minOccurs="0">
          <xs:complexType mixed="true">
            <xs:choice minOccurs="0">
              <xs:element ref="AFFile" minOccurs = "0"/>
              <xs:element name="Item" minOccurs ="0" maxOccurs ="unbounded"/>
            </xs:choice>
            <xs:attribute name="type" type="xs:string" use="optional"/>
          </xs:complexType>
        </xs:element>
        <xs:element name="AdjustedValue" minOccurs="0">
          <xs:complexType mixed="true">
            <xs:choice minOccurs="0">
              <xs:element ref="AFFile" minOccurs = "0"/>
              <xs:element name="Item" minOccurs ="0" maxOccurs ="unbounded"/>
            </xs:choice>
            <xs:attribute name="type" type="xs:string" use="optional"/>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
      <xs:attribute ref="operation" use="optional"/>
    </xs:complexType>
  </xs:element> 
  <xs:element name="AFAnalysis">
    <!-- AllReferences includes referenced
        AFAnalysisCategories
        AFAnalysisTemplate in Template
        AFElementTemplate in CaseTemplate
        AFElement in Target 
        AFSecurityIdentities from SecurityAccessControl if Security is also exported -->
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="id" minOccurs="0"/>
        <xs:element ref="Name"/>
        <xs:element ref="Description" minOccurs="0"/>
        <xs:element name="Template" type="xs:string" minOccurs="0"/>
        <xs:element name="CaseTemplate" type="xs:string" minOccurs="0"/>
        <xs:element name="OutputTime" type="xs:string" minOccurs="0"/>
        <xs:element ref="Status" minOccurs="0"/>
        <xs:element name="PublishResults" type="xs:boolean" minOccurs="0"/>
        <xs:element ref="Priority" minOccurs="0"/>
        <xs:element name="MaxQueueSize" type="xs:integer" minOccurs="0"/>
        <xs:element name="GroupID" type="xs:short" minOccurs="0"/>
        <xs:element ref="Target" minOccurs="0"/>
        <xs:element ref="AFExtendedProperty" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="AFAnalysisCategoryRef" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="AFAnalysisRule" minOccurs="0"/>
        <xs:element ref="AFTimeRule" minOccurs="0"/>
        <xs:element ref="AFCase" minOccurs="0" maxOccurs ="unbounded"/>
        <xs:element name="SecurityAccessControl" type="xs:string" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute ref="operation" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="AFAnalysisCategory">
    <xs:complexType>
      <xs:group ref="AFCategory"/>
      <xs:attribute ref="operation" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="AFAnalysisCategoryRef">
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="xs:string">
          <xs:attribute ref="id" use="optional"/>
          <xs:attribute ref="operation" use="optional"/>          
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="AFAnalysisRule">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="AFPlugIn" minOccurs ="0">
          <xs:complexType>
            <xs:simpleContent>
              <xs:extension base="xs:string">
                <xs:attribute ref="id" use="optional"/>
                <xs:attribute ref="operation" use="optional"/>
              </xs:extension>
            </xs:simpleContent>            
          </xs:complexType>
        </xs:element>
        <xs:element ref="id" minOccurs="0"/>
        <xs:element name="ConfigString" type="xs:string" minOccurs="0"/>
        <xs:element name="VariableMapping" type="xs:string" minOccurs="0"/>
        <xs:element ref="AFAnalysisRule" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute ref="operation" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="AFAnalysisTemplate">
    <!-- AllReferences includes referenced
        AFAnalysisCategories
        AFElementTemplate in CaseTemplate
        AFElement in Target 
        AFSecurityIdentities from SecurityAccessControl if Security is also exported -->
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="id" minOccurs="0"/>
        <xs:element ref="Name"/>
        <xs:element ref="Description" minOccurs="0"/>
        <xs:element name="OutputTime" type="xs:string" minOccurs="0"/>
        <xs:element name="GroupID" type="xs:short" minOccurs="0"/>
        <xs:element name="CaseTemplate" type="xs:string" minOccurs="0"/>
        <xs:element name="Target" type="xs:string" minOccurs="0"/>
        <xs:element name="CreateEnabled" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="AFExtendedProperty" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="AFAnalysisCategoryRef" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="AFAnalysisRule" minOccurs="0"/>
        <xs:element ref="AFTimeRule" minOccurs="0"/>
        <xs:element name="SecurityAccessControl" type="xs:string" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute ref="operation" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="AFAnnotation">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="id" minOccurs="0"/>
        <xs:element ref="Name" minOccurs="0"/>
        <xs:element name="Value" type="xs:string" minOccurs="0"/>
        <xs:element name="Description" type="xs:string" minOccurs="0"/>
        <xs:element name="Creator" type="xs:string" minOccurs="0" maxOccurs="1"/>
        <xs:element name="CreationDate" type="xs:dateTime" minOccurs="0" maxOccurs="1"/>
        <xs:element name="Modifier" type="xs:string" minOccurs="0" maxOccurs="1"/>
        <xs:element name="ModifyDate" type="xs:dateTime" minOccurs="0" maxOccurs="1"/>
        <xs:element name="Attachment" minOccurs="0">
          <xs:complexType>
            <xs:choice minOccurs="0">
              <xs:element ref="AFFile"/>
            </xs:choice>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
      <xs:attribute ref="operation" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="AFAttribute">
    <!-- AllReferences includes referenced AFEnumerationSets in ValueTypeQualifier when not from template-->
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="id" minOccurs="0"/>
        <xs:element ref="Name"/>
        <xs:element ref="Description" minOccurs="0"/>
        <xs:element name="IsHidden" type="xs:boolean" minOccurs="0"/>
        <xs:element name="IsManualDataEntry" type="xs:boolean" minOccurs="0"/>
        <xs:element name="IsConfigurationItem" type="xs:boolean" minOccurs="0"/>
        <xs:element name="IsExcluded" type="xs:boolean" minOccurs="0"/>
        <xs:element name="Trait" type="xs:string" minOccurs="0"/>
        <xs:element name="DefaultUOM" type="xs:string" minOccurs="0"/>
        <xs:element name="DisplayDigits" type="xs:integer" minOccurs="0"/>
        <xs:element name="Type" type="xs:string" minOccurs="0"/>
        <xs:element name="TypeQualifier" minOccurs="0">
          <xs:complexType>
            <xs:simpleContent>
              <xs:extension base="xs:string">
                <xs:attribute name="type" type="xs:string" use="optional"/>
              </xs:extension>
            </xs:simpleContent>
          </xs:complexType>
        </xs:element>
        <xs:group ref="AFValue"/>
        <xs:element name="DataReference" minOccurs="0">
          <xs:complexType>
            <xs:simpleContent>
              <xs:extension base="xs:string">
                <xs:attribute ref="id" use="optional"/>
              </xs:extension>
            </xs:simpleContent>
          </xs:complexType>
        </xs:element>
    <xs:element name="ConfigString" type="xs:string" minOccurs="0"/>
        <xs:element ref="AFAttributeCategoryRef" minOccurs="0"  maxOccurs="unbounded"/>
        <xs:element ref="AFAttribute" minOccurs="0"  maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute ref="operation" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="AFAttributeCategory">
    <xs:complexType>
      <xs:group ref="AFCategory"/>
      <xs:attribute ref="operation" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="AFAttributeCategoryRef">
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="xs:string">
          <xs:attribute ref="id" use="optional"/>
          <xs:attribute ref="operation" use="optional"/>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="AFAttributeTemplate">
    <!-- AllReferences includes referenced AFEnumerationSets in ValueTypeQualifier-->
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="id" minOccurs="0"/>
        <xs:element ref="Name"/>
        <xs:element ref="Description" minOccurs="0"/>
        <xs:element name="IsHidden" type="xs:boolean" minOccurs="0"/>
        <xs:element name="IsManualDataEntry" type="xs:boolean" minOccurs="0"/>
        <xs:element name="IsConfigurationItem" type="xs:boolean" minOccurs="0"/>
        <xs:element name="IsExcluded" type="xs:boolean" minOccurs="0"/>
        <xs:element name="IsIndexed" type="xs:boolean" minOccurs="0"/>
        <xs:element name="Trait" type="xs:string" minOccurs="0"/>
        <xs:element name="DefaultUOM" type="xs:string" minOccurs="0"/>
        <xs:element name="DisplayDigits" type="xs:integer" minOccurs="0"/>
        <xs:element name="Type" type="xs:string" minOccurs="0"/>
        <xs:element name="TypeQualifier" minOccurs="0">
          <xs:complexType>
            <xs:simpleContent>
              <xs:extension base="xs:string">
                <xs:attribute name="type" type="xs:string" use="optional"/>
              </xs:extension>
            </xs:simpleContent>
          </xs:complexType>
        </xs:element>
        <xs:element ref="Value" minOccurs="0"/>
        <xs:element name="DataReference" minOccurs="0">
          <xs:complexType>
            <xs:simpleContent>
              <xs:extension base="xs:string">
                <xs:attribute ref="id" use="optional"/>
              </xs:extension>
            </xs:simpleContent>
          </xs:complexType>
        </xs:element>
        <xs:element name="ConfigString" type="xs:string" minOccurs="0"/>
        <xs:element ref="AFAttributeCategoryRef" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="AFAttributeTemplate" minOccurs="0" maxOccurs="unbounded"/>       
      </xs:sequence>
      <xs:attribute ref="operation" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="AFCase">
    <!-- AllReferences adds 
        AFElementTemplate in Template
        AFElementCategories referenced
        AFEnumerationSets referenced in non templated attributes
        All child AFElements
        All AFConnections (not just those added or removed) -->
    <xs:complexType>
      <xs:sequence>
        <xs:group ref="AFBaseElement"/>
        <xs:element name="IsLocked" type="xs:boolean" minOccurs="0"/>
        <xs:element name="AreValuesCaptured" type="xs:boolean" minOccurs="0"/>
        <xs:element name="StartTime" type="xs:dateTime" minOccurs="0"/>
        <xs:element name="EndTime" type="xs:dateTime" minOccurs="0"/>
        <xs:element name="PreviousCase" type="xs:string" minOccurs="0"/>
        <xs:element name="CollectElementsTime" type="xs:dateTime" nillable="true" minOccurs="0"/>
        <xs:element name="CollectInputsTime" type="xs:dateTime" nillable="true" minOccurs="0"/>
        <xs:element name="CollectTransfersTime" type="xs:dateTime" nillable="true" minOccurs="0"/>
        <xs:element name="RunTime" type="xs:dateTime" nillable="true" minOccurs="0"/>
        <xs:element name="PublishTime" type="xs:dateTime" nillable="true" minOccurs="0"/>
        <xs:element name="IsPublished" type="xs:boolean" minOccurs="0"/>
        <xs:element name="CountInputs" type="xs:integer" minOccurs="0"/>
        <xs:element name="CountInputsBad" type="xs:integer" minOccurs="0"/>
        <xs:element name="CountOutputs" type="xs:integer" minOccurs="0"/>
        <xs:element name="CountOutputsBad" type="xs:integer" minOccurs="0"/>      
        <xs:element ref="Target" minOccurs="0"/>
        <xs:group ref="AFBaseElementChildren"/>
        <xs:choice minOccurs="0" maxOccurs="unbounded">
          <xs:element ref="AFElement"/>
          <xs:element ref="AFModel"/>
          <xs:element ref="AFTransfer"/>
        </xs:choice>
        <xs:element ref="AFLayerRef" minOccurs="0" maxOccurs="unbounded"/>
        <xs:choice minOccurs="0" maxOccurs="unbounded">
          <xs:element ref="AFElementRef"/>
          <xs:element ref="AFModelRef"/>
          <xs:element ref="AFTransferRef"/>
        </xs:choice>
        <xs:element ref="AFElementRemovedRef" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="AFConnection" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element name="AFConnectionRemovedRef" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="AFResult" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute ref="operation" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="AFConnection">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="id" minOccurs="0"/>
        <xs:element name="Source" type="xs:string" minOccurs="0"/>
        <xs:element name="SourcePort" type="xs:string" minOccurs="0"/>
        <xs:element name="Destination" type="xs:string" minOccurs="0"/>
        <xs:element name="DestinationPort" type="xs:string" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute ref="operation" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="AFContact">
    <!-- AllReferences adds AFSecurityIdentities from SecurityAccessControl if Security is also exported -->
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="id" minOccurs="0"/>
        <xs:element ref="Name"/>
        <xs:element name="CanonicalName" type="xs:string"  minOccurs="0"/>
        <xs:element ref="Description"  minOccurs="0"/>
        <xs:element name="FromAD" type="xs:boolean" minOccurs="0"/>
        <xs:element name="ContactType" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:enumeration value="User"/>
              <xs:enumeration value="Group"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="Department" type="xs:string" minOccurs="0"/>
        <xs:element name="Manager" type="xs:string" minOccurs="0"/>
